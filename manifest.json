{
  "name": "pr-tree-viewer",
  "version": "1.2.4",
  "manifest_version": 2,
  "description": "PR의 변경된 파일들을 트리 구조로 볼 수 있도록 도와주는 크롬 익스텐션",
  "permissions": [
    "tabs",
    "activeTab",
    "https://*/*",
    "http://*/*"
  ],
  "icons": {
    "128": "logo.png"
  },
  "content_scripts": [
    {
      "run_at": "document_idle",
      "matches": [
        "*://oss.navercorp.com/*"
      ],
      "js": [
        "index.js"
      ]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": ["*.woff2"]
}
